package currencyConverter;

public class RupeeToDollar {
	double rupee;
	double dollar;
	double dollarValue=83.72;
	
	RupeeToDollar(double rupee){
		this.rupee=rupee;
		
	}
	
	void toDollar() {
		this.dollar=this.rupee/dollarValue;	
	}

	@Override
	public String toString() {
		return "RupeeToDollar [rupee=" + this.rupee + ", dollarValue=" + this.dollarValue + "]";
	}
	
	
	
}
